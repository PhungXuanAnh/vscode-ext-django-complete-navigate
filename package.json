{
	"name": "django-complete-navigate",
	"displayName": "django-complete-navigate",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:django-complete-navigate.helloWorld",
		"onCommand:django-complete-navigate.model-objects-jump-to-definition",
		"onLanguage:python"
	],
	"main": "./out/extension.js",
	"capabilities": {
		"definitionProvider": "true"
	},
	"contributes": {
		"commands": [
			{
				"command": "django-complete-navigate.helloWorld",
				"title": "Django-complete-navigate: Hello World"
			},
			{
				"command": "django-complete-navigate.model-objects-jump-to-definition",
				"title": "Django-complete-navigate: Model object: Jump to Definition"
			}
		],
		"keybindings": [
			{
				"command": "django-complete-navigate.model-objects-jump-to-definition",
				"key": "ctrl+f1",
				"mac": "cmd+f1",
				"when": "editorTextFocus"
			}
		],
		"languages": [
			{
				"id": "python",
				"extensions": [
					".py"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"python-shell": "^3.0.0"
	}
}
